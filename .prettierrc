{
  "semi": true,
  "singleQuote": false,
  "tabWidth": 2,
  "useTabs": false,
  "printWidth": 100,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "always",
  "endOfLine": "lf",
  "plugins": ["prettier-plugin-organize-imports", "prettier-plugin-tailwindcss"],
  "organizeImports": {
    "groups": ["^react", "^@/", "^[./]"],
    "removeUnusedImports": true,
    "sortImports": true
  }
}
